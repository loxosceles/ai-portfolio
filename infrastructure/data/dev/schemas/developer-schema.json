{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "description": "Unique identifier for the developer"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "description": "Developer's full name"
    },
    "title": {
      "type": "string",
      "minLength": 1,
      "description": "Developer's professional title"
    },
    "bio": {
      "type": "string",
      "minLength": 1,
      "description": "Developer's biography"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Developer's email address"
    },
    "website": {
      "type": "string",
      "format": "uri",
      "description": "Developer's personal website"
    },
    "github": {
      "type": "string",
      "format": "uri",
      "description": "Developer's GitHub profile URL"
    },
    "linkedin": {
      "type": "string",
      "format": "uri",
      "description": "Developer's LinkedIn profile URL"
    },
    "telegram": {
      "type": "string",
      "format": "uri",
      "description": "Developer's Telegram profile URL"
    },
    "location": {
      "type": "string",
      "minLength": 1,
      "description": "Developer's location"
    },
    "yearsOfExperience": {
      "type": "number",
      "minimum": 0,
      "description": "Years of professional experience"
    },
    "isActive": {
      "type": "boolean",
      "description": "Whether the developer profile is active"
    },
    "skillSets": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1,
            "description": "Unique identifier for the skill set"
          },
          "name": {
            "type": "string",
            "minLength": 1,
            "description": "Name of the skill set category"
          },
          "skills": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string",
              "minLength": 1
            },
            "description": "List of skills in this category"
          }
        },
        "required": ["id", "name", "skills"],
        "additionalProperties": false
      },
      "description": "Developer's skill sets organized by category"
    }
  },
  "required": ["id", "name", "title", "bio", "email", "skillSets"],
  "additionalProperties": false
}
