{
  "name": "infrastructure",
  "version": "0.2.4",
  "type": "commonjs",
  "description": "",
  "bin": {
    "app": "bin/app.ts"
  },
  "scripts": {
    "build": "tsc",
    "build:cli": "ts-node lib/cli/build.ts",
    "watch": "tsc -w",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "test": "jest",
    "provision:dev": "ENVIRONMENT=dev npx cdk deploy --all --require-approval never",
    "provision:prod": "ENVIRONMENT=prod npx cdk deploy --all --require-approval=never --force",
    "destroy:dev": "ENVIRONMENT=dev cdk destroy --all --force",
    "destroy:prod": "echo 'Production destroy not allowed' && exit 1",
    "upload-ssm-params:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/ssm-params.ts upload --target=infrastructure --verbose",
    "upload-ssm-params:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/ssm-params.ts upload --target=infrastructure --verbose",
    "export-ssm-params:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/ssm-params.ts export --target=infrastructure --verbose",
    "export-ssm-params:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/ssm-params.ts export --target=infrastructure --verbose",
    "upload-static-data:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/data-management.ts upload",
    "upload-static-data:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/data-management.ts upload",
    "download-static-data:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/data-management.ts download",
    "download-static-data:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/data-management.ts download",
    "populate-static-data-ddb:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/data-management.ts populate_ddb_with_static_data --verbose",
    "populate-static-data-ddb:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/data-management.ts populate_ddb_with_static_data --verbose",
    "publish:web-app": "ENVIRONMENT=dev ts-node lib/cli/bin/web-app-publish.ts",
    "invalidate:cloudfront": "ENVIRONMENT=dev ts-node lib/cli/bin/invalidate-cloudfront-distribution.ts",
    "stack-outputs:web": "ENVIRONMENT=dev ts-node lib/cli/bin/stack-outputs.ts web",
    "sync-service-params:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/sync-service-params.ts --verbose",
    "sync-service-params:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/sync-service-params.ts --verbose",
    "sync-service-params-dry-run:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/sync-service-params.ts --dry-run --cleanup --verbose",
    "sync-service-params-dry-run:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/sync-service-params.ts --dry-run --cleanup --verbose",
    "sync-service-params-cleanup:dev": "ENVIRONMENT=dev ts-node lib/cli/bin/sync-service-params.ts --cleanup --verbose",
    "sync-service-params-cleanup:prod": "ENVIRONMENT=prod ts-node lib/cli/bin/sync-service-params.ts --cleanup --verbose",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@aws-sdk/client-cloudformation": "^3.844.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.826.0",
    "@aws-sdk/client-dynamodb": "^3.525.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-ssm": "^3.525.0",
    "@aws-sdk/lib-dynamodb": "^3.525.0",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/aws-lambda": "^8.10.150",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "aws-cdk": "^2.1020.2",
    "aws-sdk-client-mock": "^4.1.0",
    "babel-jest": "^30.0.2",
    "constructs": "^10.4.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.204.0",
    "commander": "^12.0.0",
    "constructs": "^10.0.0",
    "ts-node": "^10.9.2"
  }
}
